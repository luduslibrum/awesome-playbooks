          "ab9da6f9-0178-49de-bab0-9fed61fbe96a": {
            "nodeId": "ab9da6f9-0178-49de-bab0-9fed61fbe96a",
            "name": "Extract Asset Group Information",
            "type": "action",
            "plugin": {
              "name": "Python 3 Script",
              "slugVendor": "rapid7",
              "slugName": "python_3_script",
              "slugVersion": "2.0.2",
              "imageData": "https://eu.cdn-assets.connect.insight.rapid7.com/icons/rapid7/python_3_script/2.0.2/icon.png"
            },
            "identifier": "run",
            "continueOnFailure": false,
            "isDisabled": false,
            "isCloud": false,
            "parameters": {
              "input": {
                "function": "def run(params={}):\n    asset_groups = params.get('asset_groups')\n    \n    # ID as integer\n    id_int = int(asset_groups[0].get(\"id\"))\n    \n    # ID as list of integers\n    id_list= []\n    id_list.append(id_int)\n    \n    # Extract Console URL\n    link=asset_groups[0].get(\"links\")[0].get(\"href\")\n    url=link.split(\"/api/\")[0]\n\n    # Asset number\n    assets = asset_groups[0].get(\"assets\")\n    \n    return {\"id_list\": id_list,\n            \"id\": id_int,\n            \"url\": url,\n            \"assets\": assets}",
