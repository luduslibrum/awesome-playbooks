          "1435263f-64e3-40b2-b6ca-a49313d26468": {
            "nodeId": "1435263f-64e3-40b2-b6ca-a49313d26468",
            "name": "Convert to Dictionary and List of Assets",
            "type": "action",
            "plugin": {
              "name": "Python 3 Script",
              "slugVendor": "rapid7",
              "slugName": "python_3_script",
              "slugVersion": "2.0.2",
              "imageData": "https://eu.cdn-assets.connect.insight.rapid7.com/icons/rapid7/python_3_script/2.0.2/icon.png"
            },
            "identifier": "run",
            "continueOnFailure": false,
            "isDisabled": false,
            "isCloud": false,
            "parameters": {
              "input": {
                "function": "def run(params={}):    \n    \n    # Get input parameter\n    input = params.get('input')\n    \n    # Remove CSV header and quotation marks \n    lines = input.replace('\"',\"\").split(\"\\n\")\n    del lines[0]\n\n    # Initialize the output\n    result = []\n    collection = {}\n    assets = set()\n    \n    for line in lines:\n        splitted_line = line.split(\",\")                     # Split by comma\n        splitted_line = list(filter(None, splitted_line))   # Filter empty items        \n        print(splitted_line)\n        if len(splitted_line) > 2:                  # Only process vulnerabilities with CVEs\n            asset_id = splitted_line[0]             # Extract asset ID\n            vuln_id = splitted_line[1]              # Extract vulnerability ID\n            cves = splitted_line[2:]                # Extract list of CVEs by removing the vulnerability ID\n            cves = [cve.strip() for cve in cves]    # Remove whitespace\n        \n            if asset_id not in collection:\n                collection[asset_id] = {\n                    \"vulnids\": {},\n                    \"cves\": []\n                }\n\n            if vuln_id != \"\":\n                collection[asset_id][\"vulnids\"][vuln_id] = cves\n            if cves[0] != \"\":\n                collection[asset_id][\"cves\"].extend(cves)\n\n            assets.add(int(asset_id))\n\n    return {\"result\": collection,\n            \"assets\": list(assets)}",
                "input": {
                  "input": "{{[02b2075d-eb70-4456-8b20-921209b4df7c].[data]}}"
                }
              }
            },
