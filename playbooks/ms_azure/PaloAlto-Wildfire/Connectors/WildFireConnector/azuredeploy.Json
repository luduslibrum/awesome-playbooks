                                        "x-ms-visibility": "internal"
                                    },
                                    {
                                        "name": "body",
                                        "in": "body",
                                        "required": true,
                                        "schema": {
                                            "type": "object",
                                            "properties": {
                                                "apikey": {
                                                    "type": "string",
                                                    "description": "Paloalto Wildfire API Key",
                                                    "title": "API Key"
                                                },
                                                "url": {
                                                    "type": "string",
                                                    "description": "The URL of the web page",
                                                    "title": "URL"
                                                },
                                                "types": {
                                                    "type": "string",
                                                    "description": "Web artifact types (screenshot or download_files)",
                                                    "title": "Types"
                                                }
                                            },
                                            "required": [ "apikey", "url" ]
                                        }
                                    }
                                ]
                            }
                        },
                        "/get/pcap": {
                            "post": {
                                "responses": {
                                    "default": {
                                        "description": "default",
                                        "schema": {}
